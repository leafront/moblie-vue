inished: false,\n                list: [\n                    'kkk', 'lll'\n                ],\n                extraStatus: false\n            };\n        },\n        methods: {\n            onLoad() {\n                setTimeout(() => {\n                    this.list = [{'title': 'llll', 'key': this.list.length}]\n                    for (let i = 0; i < 10; i++) {\n                        this.list.push({'title': this.list.length + 1, 'key': this.list.length + 1});\n                    }\n                    // 加载状态结束\n                    this.loading = false;\n\n                    // 数据全部加载完成\n                    if (this.list.length >= 40) {\n                        this.finished = true;\n                    }\n                }, 500);\n            },\n            extraAction() {\n                this.extraStatus = !this.extraStatus;\n            }\n        }\n    };\n</script>\n\n<style lang=\"less\">\n\n    .wallet-list {\n        background-color: @white;\n    }\n\n    .extra-box {\n        height: 60px;\n    }\n</style>\n"]}]}